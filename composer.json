{
    "name": "workup/nova-opening-hours-field",
    "description": "Laravel Nova custom field for Spatie Opening Hours",
    "keywords": [
        "laravel",
        "nova",
        "opening hours"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2|^8.0",
        "laravel/nova": "^4.13",
        "nova-kit/nova-packages-tool": "^1.2",
        "spatie/opening-hours": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Workup\\NovaOpeningHoursField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Workup\\NovaOpeningHoursField\\FieldServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}
